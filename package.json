{
  "name": "experimental-library",
  "scripts": {
    "build": "webpack -d --config config/webpack.config.js",
    "editor-build": "cross-env NODE_ENV=es6 npm run build",
    "deploy": "cross-env NODE_ENV=production webpack -p --config config/webpack.config.js",
    "live-server": "webpack-dev-server -d --port 8080 --progress --colors --hot --config config/webpack.config.js",
    "watch": "webpack -d --config config/webpack.config.js --watch",
    "test-notWorking": "cross-env TS_NODE_COMPILER_OPTIONS='{\\\"module\\\":\\\"commonjs\\\"}' mocha test/**/*animal_test.ts",
    "test-working": "set TS_NODE_COMPILER_OPTIONS={\"module\":\"commonjs\"} && mocha test/**/*_test.ts",
    "test-crossEnv": "cross-env-shell TS_NODE_COMPILER_OPTIONS='{\\\"module\\\":\\\"commonjs\\\"}' node test/core/cross-env_test.ts"
  },
  "devDependencies": {
    "@types/chai": "^3.5.2",
    "@types/mocha": "^2.2.41",
    "@types/node": "^7.0.18",
    "babel-cli": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "chai": "^3.5.0",
    "cross-env": "^5.0.0",
    "mocha": "^3.4.1",
    "ts-node": "^3.0.4",
    "webpack": "^2.5.1",
    "webpack-notifier": "^1.5.0",
    "webpack-dev-server": "^2.4.5",
    "ts-loader": "^2.0.3",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "typescript": "^2.3.2"
  },
  "dependencies": {
    "babel-runtime": "^6.23.0"
  }
}
