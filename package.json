{
  "name": "experimental-library",
  "scripts": {
    "build": "webpack -d --config config/webpack.config.js",
    "editor-build": "cross-env NODE_ENV=es6 npm run build",
    "deploy": "cross-env NODE_ENV=production webpack -p --config config/webpack.config.js",
    "live-server": "webpack-dev-server -d --port 8080 --progress --colors --hot --config config/webpack.config.js",
    "watch": "webpack -d --config config/webpack.config.js --watch",
    "test-notWorking": "cross-env TS_NODE_COMPILER_OPTIONS={\"module\":\"commonjs\"} && mocha test/**/*_test.ts",
    "test-working": "set TS_NODE_COMPILER_OPTIONS={\"module\":\"commonjs\"} && mocha test/**/*_test.ts"
  },
  "devDependencies": {
    "@types/chai": "^3.5.2",
    "@types/mocha": "^2.2.41",
    "babel-cli": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "chai": "^3.5.0",
    "cross-env": "^4.0.0",
    "mocha": "^3.4.1",
    "ts-node": "^3.0.4",
    "webpack-dev-server": "^2.4.5"
  },
  "dependencies": {
    "babel-runtime": "^6.23.0"
  }
}
